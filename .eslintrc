{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "arrow-parens": "off",
    "no-param-reassign": "off",
    "max-len": ["error", 80],
    "max-lines-per-function": ["error", 40]
  },
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 10
  },
  "extends": ["airbnb-base"]
}
